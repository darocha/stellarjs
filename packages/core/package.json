{
  "name": "@stellarjs/core",
  "version": "0.2.0",
  "main": "src/index.js",
  "browser": "dist/index.browser.js",
  "src": [
    "src/**/*"
  ],
  "scripts": {
    "pretest": "yarn run build",
    "test": "NODE_ENV=test mocha",
    "test-coverage": "rm -rf coverage/* && nyc --cache --reporter=html --reporter=text-lcov mocha",
    "ci": "yarn run test-coverage && codecov < coverage/coverage-remapped.lcov",
    "build": "git rev-parse HEAD > .commithash && webpack -p",
    "watch:browser": "webpack -d --watch",
    "prepublish": "yarn run build",
    "lint": "eslint src"
  },
  "repository": "stellarjs/stellar",
  "keywords": [
    "microservices",
    "es6",
    "stellar",
    "stellarjs"
  ],
  "author": "Andres Olave",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sloops77/stellarjs/issues"
  },
  "devDependencies": {
    "acorn": "4.0.4",
    "chalk": "^1.1.3",
    "codecov.io": "^0.1.6",
    "console-group": "^0.3.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^0.4.3",
    "locate-character": "^2.0.0",
    "mocha": "^3.0.0",
    "nyc": "^10.3.2",
    "remap-istanbul": "^0.6.4",
    "require-relative": "^0.8.7",
    "source-map": "^0.5.6",
    "sourcemap-codec": "^1.3.0"
  },
  "dependencies": {
    "bluebird": "3.4.6",
    "lodash": "4.15.0",
    "uuid": "^3.0.1"
  }
}
