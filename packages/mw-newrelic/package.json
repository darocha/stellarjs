  },
  "devDependencies": {
    "acorn": "4.0.4",
    "chalk": "^1.1.3",
    "codecov.io": "^0.1.6",
    "console-group": "^0.3.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^0.4.3",
    "locate-character": "^2.0.0",
    "mocha": "^3.0.0",
    "nyc": "^10.3.2",
    "remap-istanbul": "^0.6.4",
    "require-relative": "^0.8.7",
    "source-map": "^0.5.6",
    "sourcemap-codec": "^1.3.0"
  },
  "dependencies": {
    "@stellarjs/core": "^0.2.2",
    "bluebird": "3.4.6",
    "lodash": "4.15.0",
    "newrelic": "^1.36.0"
