{
  "name": "@stellarjs/mw-newrelic",
  "version": "0.2.2",
  "main": "dist/index.js",
  "src": [
    "src/**/*"
  ],
  "scripts": {
    "pretest": "yarn run build",
    "test": "NODE_ENV=test mocha",
    "test-coverage": "rm -rf coverage/* && nyc --cache --reporter=html --reporter=text-lcov mocha",
